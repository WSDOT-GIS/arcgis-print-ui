{
    "exclude": [
        "node_modules"
    ],
    "compilerOptions": {
        "module": "amd",
        "target": "es5",
        "lib": [
            "es5",
            "dom",
            "es2015.promise",
            "es2015.iterable",
            "es2015.symbol.wellknown",
            "es2015.symbol"
        ],
        "noImplicitAny": true,
        "strictNullChecks": true,
        "sourceMap": false
    }
}